{
  "name": "kingjames",
  "version": "0.0.14",
  "description": "A King James Bible search library, currently no database requried",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "jest --watch",
    "test:cov": "jest --coverage",
    "test:ci": "jest --ci --reporters='default' --reporters='./.github/reporters/github-actions-reporter'",
    "build": "prisma generate && prisma db push && prisma db seed && rm -rf ./dist && tsc && mkdir ./dist/prisma && cp ./src/prisma/schema.prisma ./dist/prisma/schema.prisma",
    "prepublish": "tsc"
  },
  "keywords": [],
  "author": "Robert Crossland",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^4.16.2",
    "fp-ts": "^2.13.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@types/node": "^18.16.18",
    "jest": "^29.4.1",
    "prisma": "^4.16.2",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testPathIgnorePatterns": [
      "dist"
    ]
  },
  "prisma": {
    "seed": "ts-node ./src/seed/seed.ts",
    "schema": "./src/prisma/schema.prisma"
  }
}
